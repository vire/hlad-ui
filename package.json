{
  "name": "hlad-ui",
  "version": "0.1.0",
  "description": "Web client for the hlad lunch crawler app",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --colors --config webpack.conf.js",
    "lint": "tslint -c tslint.json src/**/*.tsx src/**/*.ts",
    "postinstall": "typings install",
    "start": "webpack-dev-server --config webpack.conf.js --content-base . --history-api-fallback --hot --inline --port 3000",
    "test": "NODE_ENV=test karma start karma.conf.js",
    "test:dev": "npm run test -- --single-run=false"
  },
  "keywords": [
    "hlad",
    "cralwer",
    "webui"
  ],
  "author": "vire <code.vire@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vire/hlad-ui.git"
  },
  "devDependencies": {
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "karma": "^1.2.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lolex": "^1.4.0",
    "mocha": "^3.0.2",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.6",
    "postcss-loader": "^0.10.1",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "tslint": "^3.8.1",
    "tslint-loader": "^2.1.4",
    "typescript": "^1.8.10",
    "typings": "^1.3.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "core-decorators": "^0.12.3",
    "dotenv": "^2.0.0",
    "firebase": "^2.4.1",
    "immutable": "^3.7.6",
    "js-yaml": "^3.6.1",
    "node-uuid": "^1.4.7",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-observable": "^0.9.1",
    "rxjs": "^5.0.0-beta.11"
  }
}
